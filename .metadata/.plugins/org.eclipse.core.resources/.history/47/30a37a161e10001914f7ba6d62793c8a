package p6;

public class Test {
	public static void main(String[] args) {
		testArray7x7();
	}
	
	private static void testArray7x7() {
		Array7x7 array7x7 = new Array7x7();
		
		Array7 array7 = new Array7(new int[] { 1, 2, 3, 4, 5, 6, 7 });
		for (int i = 0; i < 7; i++)
			System.out.print(array7.getElement(i));
	}
}
